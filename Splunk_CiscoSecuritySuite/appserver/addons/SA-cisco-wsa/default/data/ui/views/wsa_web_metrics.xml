<dashboard stylesheet="application.css" onUnloadCancelJobs="true">
	<label>Web Request Metrics</label>

	<row>
		<chart>
			<title>Average Request Size (Allowed Requests)</title>
			<searchName>Cisco WSA - Web Request Metrics - Average Request Size</searchName>
			<option name="charting.chart">area</option>
			<option name="charting.chart.nullValueMode">zero</option>
			<option name="charting.axisTitleY.visibility">visible</option>
			<option name="charting.axisTitleY.text">Request Size</option>
			<option name="charting.axisTitleX.visibility">collapsed</option>
			<option name="charting.legend.placement">right</option>
		</chart>
		<chart>
			<title>Unique Users Over Time</title>
			<searchName>Cisco WSA - Web Request Metrics - Unique Users</searchName>
			<option name="charting.chart">line</option>
			<option name="charting.chart.nullValueMode">zero</option>
			<option name="charting.axisTitleY.visibility">visible</option>
			<option name="charting.axisTitleY.text"># Unique Users</option>
			<option name="charting.axisTitleX.visibility">collapsed</option>
			<option name="charting.legend.placement">none</option>
		</chart>
	</row>
	<row>
		<chart>
			<title>Cache Efficiency</title>
			<searchName>Cisco WSA - Web Request Metrics - Cache Efficiency</searchName>
			<option name="charting.chart">pie</option>
			<option name="charting.legend.placement">right</option>
		</chart>
		<chart>
			<title>Users with Multiple User-Agents</title>
			<searchName>Cisco WSA - Web Request Metrics - Users with Multiple UAs</searchName>
			<option name="charting.chart">pie</option>
			<option name="charting.legend.placement">right</option>
		</chart>
		<chart>
			<title>Users with Multiple Client IPs</title>
			<searchName>Cisco WSA - Web Request Metrics - Users with Multiple IPs</searchName>
			<option name="charting.chart">pie</option>
			<option name="charting.legend.placement">right</option>
		</chart>
	</row>
</dashboard>
