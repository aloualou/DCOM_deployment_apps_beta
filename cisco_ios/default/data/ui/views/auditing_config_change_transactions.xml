<?xml version='1.0' encoding='utf-8'?>
<form>
  <label>Configuration change transactions</label>
  <searchTemplate>| savedsearch "change management transactions" hostToken="$hostToken$"</searchTemplate>
  <fieldset autoRun="true">
    <input type="text" token="hostToken">
      <label>Host</label>
      <default>*</default>
    </input>
    <input type="time">
      <default>Last 60 minutes</default>
    </input>
  </fieldset>
  <row>
    <table>
      <title>Configuration change transactions</title>
      <option name="showPager">true</option>
      <option name="count">20</option>
    </table>
  </row>
</form>
